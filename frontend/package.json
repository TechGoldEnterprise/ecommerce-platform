    "build:prod": "node scripts/build-windows.js --production",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prebuild": "echo 'Use npm run build instead'",
    "generate-env": "node scripts/generate-env.js",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "netlify-build": "npm run generate-env && npm run generate-sitemap && set NODE_OPTIONS=--openssl-legacy-provider && react-scripts build"
}
,
